   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(1, 1000, 150)\n",
    "y = np.log(x)\n",
    "\n",
    "plt.plot(x, y)\n",
    "\n",
    "# Get Current Axes (=gca)\n",
    "axes = plt.gca()\n",
    "axes.set_yscale(\"log\")"
   ]
